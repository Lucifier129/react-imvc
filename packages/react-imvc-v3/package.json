{
  "name": "react-imvc",
  "version": "3.4.5",
  "description": "An Isomorphic MVC Framework",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "react-imvc": "./bin/scripts.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/Lucifier129/react-imvc",
  "scripts": {
    "build": "tsc",
    "prebuild": "rimraf ./dist",
    "pretest": "tsc",
    "test": "jest --runInBand --forceExit --coverage",
    "format": "prettier --write --parser typescript \"{src,test}/**/*.ts?(x)\"",
    "test:start": "ts-node ./project/start.ts",
    "test:debug": "node -r ts-node/register --inspect-brk ./project/start.ts",
    "publish-imvc": "npm publish",
    "prepublish-imvc": "npm run build",
    "publish-beta": "npm publish --tag beta",
    "rmnm": "rimraf node_modules",
    "posttest:coverage": "nyc report",
    "pretest:coverage": "rimraf coverage playwright-report .nyc_output",
    "test:coverage": "playwright test",
    "project:start": "node --openssl-legacy-provider .\\bin\\scripts.js start --config .\\project\\imvc.config.ts",
    "project:build": "node --openssl-legacy-provider .\\bin\\scripts.js build?inspect-brk --config .\\project\\imvc.config.ts",
    "project:preview": "npx tsx --openssl-legacy-provider publish/start"
  },
  "files": [
    "bin",
    "build",
    "component",
    "config",
    "context",
    "controller",
    "entry",
    "hoc",
    "hook",
    "middleware",
    "page",
    "start",
    "util",
    "dist",
    "playwright",
    "tsconfig.json"
